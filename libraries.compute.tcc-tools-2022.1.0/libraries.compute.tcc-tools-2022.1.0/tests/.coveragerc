# .coveragerc to control coverage.py
[run]
branch = True
omit =
    */.local/*
    /usr/*
    */test_*

[report]
exclude_lines =
    # Have to re-enable the standard pragma
    pragma: no cover
    def __repr__
    if self\.debug
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:

ignore_errors = True

[html]
directory = _coverage_html_report
