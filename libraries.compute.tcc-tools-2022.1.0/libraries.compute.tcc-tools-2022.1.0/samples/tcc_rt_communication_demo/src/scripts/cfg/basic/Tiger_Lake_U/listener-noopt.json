{
  "eth-if": {
    "ip": {
      "interface": "${interface}",
      "if-address": "169.254.1.22",
      "if-net-bits-count": 24,
      "if-broadcast-address": "169.254.1.255",
      "if-mac-address": "22:bb:22:bb:22:bb",
      "vlan-id": 3,
      "vlan-address": "169.254.11.22",
      "vlan-net-bits-count": 24,
      "vlan-broadcast-address": "169.254.11.255"
    },
    "ethtool": {
      "interface": "${interface}",
      "rx": 4,
      "tx": 4
    },
    "irq-affinity": {
      "interface": "${interface}",
      "file": "${config_path}/common/irq_affinity_4c_4tx_4rx.map"
    }
  },
  "tsn-disc": {
    "interface": "${interface}",
    "taprio": {
      "handle": 100,
      "num_tc": 4,
      "queues": "1@0 1@1 1@2 1@3",
      "time_elapsed": 0,
      "mapping": {
        "default": 0,
        "p1": 1,
        "p2": 2,
        "p3": 3
      },
      "schedule": [
        {
          "gate_mask": "0f",
          "duration": 250000
        },
        {
          "gate_mask": "0e",
          "duration": 250000
        }
      ],
      "offload": false
    },
    "etf": [
      {
        "delta": 15000,
        "queue": 3,
        "offload": true
      }
    ],
    "vlanrx": [
      {
        "vlan_priority": 1,
        "rx_hw_q": 1
      },
      {
        "vlan_priority": 2,
        "rx_hw_q": 2
      },
      {
        "vlan_priority": 3,
        "rx_hw_q": 3
      }
    ]
  },
  "sync-clocks": {
    "ptp4l": {
      "interface": "${interface}.vlan",
      "ignore_existing": false,
      "gPTP_file": "${config_path}/ptp/gPTP_SGMII-MV2110-1G.cfg",
      "socket_prio": 2
    },
    "phc2sys": {
      "interface": "${interface}.vlan",
      "clock": "CLOCK_REALTIME",
      "ignore_existing": false
    }
  },
  "stress-apps": {
    "best-effort": {
      "role": "server"
    }
  },
  "run-apps": {
    "opcua_server": {
      "publisher_interface": "${interface}",
      "subscriber_interface": "${interface}",
      "use_xdp": false,
      "packet_count": 2000,
      "cycle_time_ns": 500000,
      "polling_duration_ns": 0,
      "publishers": {},
      "subscribers": {
        "sub1": {
          "url": "opc.eth://22-bb-22-bb-22-bb",
          "sub_id": 0,
          "subscribed_pub_id": 2234,
          "subscribed_dataset_writer_id": 62541,
          "subscribed_writer_group_id": 101,
          "two_way_data": false,
          "cpu_affinity": 3,
          "xdp_queue": -1,
          "offset_ns": 350000
        }
      },
      "tcc_settings": {
        "latency": 250,
        "affinity": 2
      },
      "listener": {
        "affinity": 3,
        "iterations": 2000,
        "output-file": "${output_file}",
        "start-offset": 375000,
        "flavor": "talker",
        "period": 500000
      }
    },
    "tcpdump": "${interface}",
    "carrier": "opcua_server",
    "carrier-args": "--config-file=${output_path}/listener-noopt.json --base-time ${base_time}"
  },
  "finish-apps": {
  }
}
