{
  "description": "This is example of config for opcua_server for AF_PACKET basic-b profile",
  "run-apps": {
    "opcua_server": {
      "publisher_interface": "${interface}",
      "subscriber_interface": "${interface}",
      "use_xdp": false,
      "packet_count": 100000,
      "cycle_time_ns": 250000,
      "polling_duration_ns": 100000,
      "publishers": {},
      "subscribers": {
        "sub1": {
          "url": "opc.eth://22-bb-22-bb-22-bb",
          "sub_id": 0,
          "subscribed_pub_id": 2234,
          "subscribed_dataset_writer_id": 62541,
          "subscribed_writer_group_id": 101,
          "two_way_data": false,
          "cpu_affinity": 2,
          "xdp_queue": -1,
          "offset_ns": 0
        }
      },
      "tcc_settings": {
        "latency": ${tcc_latency}
      },
      "listener": {
        "iterations": 2000,
        "output-file": "${output_file}",
        "start-offset": 30000,
        "flavor": "talker",
        "period": 250000
      }
    }
  }
}

