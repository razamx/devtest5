<configuration>
  <appSettings>

    <add key="-tmpdir" value="/tmp/" />
    <!--<add key="-debuglevel" value="[DEBUG_LEVEL]"/>-->
    <!--Values: 0 - minimal, 1 - medium, 2 - detailed-->

    <!-- Drop Service URL:-->
    <!--add key="-swfurl" value="http://softwarefactory.intel.com/bissvc" /--> <!-- Software Factory drop service -->

    <add key="-swfurl" value="http://phoenix.intel.com/BisSvc/" />  <!-- Phoenix drop service -->
    <add key="-performafterlocal" value="echo OK"   />
    <add key="-rsyncmodulepath" value="swfrepos" />
    <add key="-dircreationpattern" value='if [ ! -d %Directory% ]; then mkdir -p %Directory% ; fi;'  />

 <!-- Copy commands -->
    <add key="-copypattern" value="rsync -rpOtlI %LocalSourceDirectory%/%TimeStamp%/%DropArea%/%Directory% /%ReplicationShare%/%ProductPhaseDirectory%/%DropArea%/ " />
    <add key="-bincopypattern" value="rsync -rpOtlI %LocalSourceDirectory%/%TimeStamp%/%DropArea%/%Directory% /%ReplicationShare%/%ProductPhaseDirectory%/%DropArea%/ " />
 <!-- Correct permission -->
    <add key="-performafterdelivery" value='cd /%ReplicationShare%/%ProductPhaseDirectory%/%DropArea%/   ;find . -type d -exec chmod 2775 {} \; ; find . -type f -exec chmod ug+rw {} \;' />

  </appSettings>

</configuration>

