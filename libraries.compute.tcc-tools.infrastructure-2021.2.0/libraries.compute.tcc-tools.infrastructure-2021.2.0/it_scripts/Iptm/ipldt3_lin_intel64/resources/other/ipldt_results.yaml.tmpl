program_name: {$programName}
program_version: {$programVersion}
launch_parameters: {$launchParameters}
string_file_version: {$stringFileVersion}
scan_completed: {$scanCompleted}
{% if highExposureFound == "yes" %}high_exposure_found: true{% endif %}
{% if highExposureFound != "yes" %}high_exposure_found: false{% endif %}
summary:{% for hitCount in hitCounts.hitCountsList %}
  - string: {$hitCount.hit}
    category: {$hitCount.category}
    section: {$hitCount.section}
    count: {$hitCount.count}
    filenames:{% for filename in hitCount.filenames %}
    - name: {$filename.name}
      count: {$filename.count}{% endfor %}
{% endfor %}
details:{% for hitInstance in hitDetails.hitInstanceList %}
  - string: {$hitInstance.name}
    section: {$hitInstance.section}
    guidance: {$hitInstance.guidance}
    contexts:{% for hitContext in hitInstance.hitContextList %}
      - file: {$hitContext.file}
        contexts:
          - count: {$context.count}
            lines:{% for line in context.lines %}
              - {$line}{% endfor %}
            before: "{$context.before}"
            hits:{% for hitAndAfter in context.hitsAndAfter %}
              - "{$hitAndAfter.hit}"{% endfor %}
            after:{% for hitAndAfter in context.hitsAndAfter %}
              - "{$hitAndAfter.after}"{% endfor %}{% endfor %}
{% endfor %}
statistics:{% for statisticsItem in statistics %}
  {$statisticsItem.name}: {$statisticsItem.value}{% endfor %}
